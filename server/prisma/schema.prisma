generator client {
  provider        = "prisma-client"
  output          = "./generated"
  previewFeatures = ["typedSql"]
  engineType      = "client"
}

datasource db {
  provider = "postgresql"
}

model User {
  id       Int       @id @default(autoincrement())
  email    String    @unique
  password String?
  accounts Account[]
}

model Account {
  id       Int    @id @default(autoincrement())
  user     User   @relation(fields: [userId], references: [id])
  userId   Int
  provider String
}
